  "name": "@wheelroom/boilerplate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheelroom/wheelroom.git"
  },
  "scripts": {
    "compile-config": "npm-run-all compile:*",
    "compile:copy-fragments": "npm run copy-fragments",
    "compile:config": "tsc --project src/wheelroom-config/tsconfig.json",
    "compile": "npm-run-all compile:*",
    "copy-fragments": "./copy-graphql-fragments.sh",
    "demo-load-env": "export $(grep -v '^#' .env | xargs)",
    "demo-postversion": "git push && git push --tags",
    "demo-version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "lint-fix": "eslint 'src*/**/*.{js,ts,tsx}' --fix",
    "lint": "eslint 'src*/**/*.{js,ts,tsx}'",
    "netlify:fragments": "npm run copy-fragments",
    "netlify:gatsby": "gatsby build",
    "netlify": "npm-run-all netlify:*",
    "start": "npm run develop",
    "svgr": "svgr --typescript --filename-case kebab --ext tsx --no-dimensions --out-dir src/svg/figma src/svg/figma --config-file ./src/svg/svgr.config.js",
    "type-check": "tsc --noEmit",
    "version": "node sync-wheelroom-version.js",
    "wr": "wheelroom --config compiled-config/wheelroom-config.js",
    "wr:cm": "wheelroom --config compiled-config/wheelroom-config.js create-models",
    "wr:dm": "wheelroom --config compiled-config/wheelroom-config.js delete-models",
    "wr:cc": "wheelroom --config compiled-config/wheelroom-config.js create-content",
    "wr:dc": "wheelroom --config compiled-config/wheelroom-config.js delete-content",
    "wr:cb": "wheelroom --config compiled-config/wheelroom-config.js create-boilerplate react src/wheelroom/models",
    "wr:cg": "wheelroom --config compiled-config/wheelroom-config.js create-graphql gatsbyjs src/wheelroom/models",
    "wr:ls": "wheelroom --config compiled-config/wheelroom-config.js list"
  },
  "type": "commonjs",
  "version": "5.0.5",
  "wheelroom": {
    "version": "5.0.5"
  }
}
